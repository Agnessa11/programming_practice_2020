import turtle

turtle.shape('turtle')

n = 10
for i in range(10):
    turtle.pendown()
    turtle.forward(n*(2*i + 1))
    turtle.left(90)
    turtle.forward(n*(2*i + 1))
    turtle.left(90)
    turtle.forward(n*(2*i + 1))
    turtle.left(90)
    turtle.forward(n*(2*i + 1))
    turtle.right(45)
    turtle.penup()
    turtle.forward(14)
    turtle.left(135)

import turtle
turtle.shape('turtle')
turtle.color('green')
n = 10
turtle.right(90 + 180/n)
for i in range(1, n + 1):
    turtle.forward(100)
    turtle.right(180 - 180/n)
turtle.penup()
turtle.goto(200, 0)
turtle.pendown()

n = 11
turtle.right(90 + 180/n)
for i in range(1, n + 1):
    turtle.forward(100)
    turtle.right(180 - 180/n)
turtle.right(3600
             )